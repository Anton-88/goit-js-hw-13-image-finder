{{#each this}}
<li class="gallery__item" >
    <div class="photo-card">
        <img class="photo-card-img" src="{{webformatURL}}" width= "360" alt="{{tags}}" data-largeImg ="{{largeImageURL}}" "/>
        <div class="stats">
            <p class="stats-item"> 
                <i class="material-icons">thumb_up</i>
                likes: {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                views: {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                comments: {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                downloads: {{downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}